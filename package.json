{
  "name": "image-compress-tool",
  "version": "1.0.0",
  "description": "A desktop application for compressing images",
  "main": "electron/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "electron .",
    "build": "vite build && electron-builder"
  },
  "keywords": [
    "image",
    "compression",
    "electron",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^6.5.1",
    "electron": "^13.6.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^2.9.15"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0"
  }
}
